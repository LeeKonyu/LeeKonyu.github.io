    <span class="nt">&lt;meta</span> <span class="na">http-equiv=</span><span class="s">"X-UA-Compatible"</span> <span class="na">content=</span><span class="s">"IE=edge"</span><span class="nt">&gt;</span>
    <span class="nt">&lt;title&gt;&lt;/title&gt;</span>
    <span class="nt">&lt;meta</span> <span class="na">name=</span><span class="s">"description"</span> <span class="na">content=</span><span class="s">""</span><span class="nt">&gt;</span>
    <span class="nt">&lt;meta</span> <span class="na">name=</span><span class="s">"viewport"</span> <span class="na">content=</span><span class="s">"width=device-width"</span><span class="nt">&gt;</span>

  <span class="nt">&lt;/head&gt;</span>
  <span class="nt">&lt;body</span> <span class="na">ng-app=</span><span class="s">"fasterAngular"</span><span class="nt">&gt;</span>
    <span class="nt">&lt;h1&gt;</span>Fast AngularJs<span class="nt">&lt;/h1&gt;</span> <span class="nt">&lt;h3&gt;</span>AngularJs with ReactJs<span class="nt">&lt;/h3&gt;</span>
    <span class="nt">&lt;div</span> <span class="na">ng-controller=</span><span class="s">"mycontroller"</span><span class="nt">&gt;</span>

        <span class="nt">&lt;fast-repeat</span> <span class="na">data=</span><span class="s">"data"</span><span class="nt">&gt;&lt;/fast-repeat&gt;</span>

    <span class="nt">&lt;/div&gt;</span>

    <span class="nt">&lt;script </span><span class="na">src=</span><span class="s">"bower_components/angular/angular.js"</span><span class="nt">&gt;&lt;/script&gt;</span>
    <span class="nt">&lt;script </span><span class="na">src=</span><span class="s">"bower_components/react/react.js"</span><span class="nt">&gt;&lt;/script&gt;</span>

