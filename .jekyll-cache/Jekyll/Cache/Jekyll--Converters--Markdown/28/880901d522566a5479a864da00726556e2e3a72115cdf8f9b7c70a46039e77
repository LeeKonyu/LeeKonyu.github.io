        <span class="kd">var</span> <span class="nx">MYLIST</span> <span class="o">=</span> <span class="nx">React</span><span class="p">.</span><span class="nx">createClass</span><span class="p">({</span><span class="na">displayName</span><span class="p">:</span> <span class="dl">'</span><span class="s1">MYLIST</span><span class="dl">'</span><span class="p">,</span>
